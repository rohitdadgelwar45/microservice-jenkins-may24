pipeline{
    agent any
   tools{
        nodejs='nodejs'
        }

    stages{
        stage("Git Checkout"){
            steps{
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/rohitdadgelwar45/microservice-jenkins-may24.git']])
            }
            
        }
        
        
    }
} 
